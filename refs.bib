@InProceedings{Mannam2021,
  author    = {Mannam, Pragna and Kroemer, Oliver and Temel, F. Zeynep},
  booktitle = {Experimental {Robotics}},
  title     = {Characterization of {Compliant} {Parallelogram} {Links} for {3D}-{Printed} {Delta} {Manipulators}},
  year      = {2021},
  address   = {Cham},
  editor    = {Siciliano, Bruno and Laschi, Cecilia and Khatib, Oussama},
  pages     = {75--84},
  publisher = {Springer International Publishing},
  series    = {Springer {Proceedings} in {Advanced} {Robotics}},
  abstract  = {We design a compliant delta manipulator using 3D-printing and soft materials.  Our design is different from the traditionally rigid delta robots as it is more accessible through low-cost 3D-printing, and can interact safely with its surroundings due to compliance. This work focuses on parallelogram links which are a key component of the delta robot design. We characterize these links over twelve dimensional parameters, such as beam and hinge thickness, and two material stiffness settings by displacing them, and observing the resulting forces and rotation angles. The parallelogram links are then integrated into a delta robot structure to test for delta mechanism behavior, which keeps the end-effector parallel to the base of the robot. We observed that using compliant hinges resulted in near-delta behavior, laying the groundwork for fabricating and utilizing 3D-printed compliant delta manipulators.},
  doi       = {10.1007/978-3-030-71151-1_7},
  file      = {:Mannam2021 - Characterization of Compliant Parallelogram Links for 3D Printed Delta Manipulators.pdf:PDF},
  isbn      = {9783030711511},
  language  = {en},
}

