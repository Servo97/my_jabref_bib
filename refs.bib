@Article{Freedy1973,
  author   = {Freedy, Amos},
  journal  = {Joint Automatic Control Conference},
  title    = {Learning control in remote manipulator and robot systems},
  year     = {1973},
  number   = {11},
  pages    = {787},
  abstract = {Robots and remote manipulators are anthropometric systems designed to extend man's mobility and manipulative functions to remote locations and unknown environments. These are general purpose devices and must have capabilities for sensing the environment of operation, recognizing patterns and autonomously making control decisions. Two types of systems are recognized: (1) man-supervised semi-autonomous systems, and (2) completely autonomous robot systems. The first type involves dexterous manipulation under human supervision in hazardous and/or remote environments. The second type is usually mobile goal-directed devices which are able to interact independently with their environment and accomplish a set of task objectives in response to high-level commands. This paper introduces the principles and functional organization of control systems of robots and manipulator systems and emphasizes the unique role of learning in system operation. Of particular interest are the properties of trainable control policies which provide the ability to change behavioral patterns with changing environmental statistics and allow performance improvement as experience is acquired. Learning techniques also provide a capability for acquiring human decision making and control policies in an "on the job" mode. That is, the system can observe direct human control action and take over control when training is completed. The potential of combining learning networks with sets of heuristic programs is also of importance. Current applications of learning techniques in robots and manipulators are described. An example of an interactive man-learning control system is discussed, along with results of experimental work. A brief exploratory discussion about the role of learning techniques in future robot and manipulator systems is given. Practical considerations regarding the implementation of learning control systems in real world operations are also discussed.},
  doi      = {10.1109/JACC.1973.4169962},
  language = {English},
  url      = {https://www.infona.pl//resource/bwmeta1.element.ieee-art-000004169962},
  urldate  = {2023-05-06},
}

